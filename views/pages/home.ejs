<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://cdn.tailwindcss.com"></script>
</head>
<body class=" flex items-center justify-center" style="background: #edf2f7;">
  <div class="flex items-center justify-center p-12 w-full">
  <div class="mx-auto w-full max-w-[650px] bg-white">
    <form
      class="py-6 px-9"
      action="/api/upload"
      method="POST"
      enctype="multipart/form-data"
    >

      <div class="mb-6 pt-4">
        <label class="mb-5 block text-xl font-semibold text-[#07074D]">
          Upload File
        </label>
        <label class="mb-5 block text-s font-semibold text-[#07074D]">
          <%= messages %>
        </label>
        <div class="mb-8">
          <input type="file" name="file" id="file"  />
        </div>
        <% objects.forEach(function(file) { %>
        <div class="mb-5 rounded-md bg-[#F5F7FB] py-4 px-8">
          <div class="flex items-center justify-between">
            <span class="truncate pr-3 text-base font-medium text-[#07074D]">
              <%= file.Key %>
            </span>
          </div>
        </div>
        <% }); %>
      </div>

      <div>
        <button
          class="hover:shadow-form w-full rounded-md bg-[#6A64F1] py-3 px-8 text-center text-base font-semibold text-white outline-none"
        >
          Send File
        </button>
        
      </div>
    </form>
  </div>
</div>
</body>
</html>